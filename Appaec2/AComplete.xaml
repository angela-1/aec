<Page x:Uid="Page_1" x:Class="Appaec2.AComplete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Appaec2"
      mc:Ignorable="d" 
      d:DesignHeight="327" d:DesignWidth="308"
      Title="">

    <Grid x:Name="grid" x:Uid="Grid_1">
        <ListBox x:Uid="listBox" x:Name="listBox" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" ItemsSource="{Binding Accountlist}" ItemTemplate="{DynamicResource ListBoxDataTemplate}" ItemContainerStyle="{DynamicResource ListBoxItemStyle}" Width="308" Height="327" SelectedIndex="{Binding Selectedindex}">
            <ListBox.DataContext>
                <local:ACompleteViewModel/>
            </ListBox.DataContext>

        </ListBox>



    </Grid>


</Page>
